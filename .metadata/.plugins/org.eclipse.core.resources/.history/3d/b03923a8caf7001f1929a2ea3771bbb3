#include "initialize.h"

void initialize(void) {

	// create the pointers to the memory
	// first pointer should be the clock
	RCC_AHB1ENR_t *pToGPIOAClock;
	pToGPIOAClock = (RCC_AHB1ENR_t *) 0x40023830;

	pToGPIOAClock->GPIOAEN = 1;



}
